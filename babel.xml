<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
<corpora>
	<crawls>
		<Path>/mnt/data/babel/Resources/Crawls/Crawls.ur-en.tiny</Path>
		<SrcSubDir>ur/</SrcSubDir>
		<TrgSubDir>en/</TrgSubDir>
		<DateFrom>01-01-01</DateFrom>
		<DateTo>01-01-30</DateTo>
		<OneSentPerLine>true</OneSentPerLine>
	</crawls>
	<wiki>
		<Path>/mnt/data/babel/Resources/babel/wiki</Path>
		<SrcRegExp>.*\\.es\\.small</SrcRegExp>
		<TrgRegExp>.*\\.en\\.small</TrgRegExp>
		<OneSentPerLine>false</OneSentPerLine>
	</wiki>
	<europarl>
		<Path>/mnt/data/babel/Resources/EuroParl/ver.5/es-en/train.small</Path>
		<SrcSubDir>es/</SrcSubDir>
		<TrgSubDir>en/</TrgSubDir>
		<DateFrom>96-04-15</DateFrom>
		<DateTo>09-10-23</DateTo>
		<OneSentPerLine>true</OneSentPerLine>
	</europarl>
	<dev>
		<Path>/mnt/data/babel/Resources/babel/EuroParl/ver.5/es-en/dev</Path>
		<SrcName>dev.es</SrcName>
		<TrgName>dev.en</TrgName>
		<OneSentPerLine>true</OneSentPerLine>
	</dev>
	<test>
		<Path>/mnt/data/babel/Resources/babel/EuroParl/ver.5/es-en/test</Path>
		<SrcName>test.es</SrcName>
		<TrgName>test.en</TrgName>
		<OneSentPerLine>true</OneSentPerLine>
	</test>
</corpora>
<resources>
	<stopwords>
		<Path>/mnt/data/babel/Resources/StopWordLists/</Path>
		<TrgStopWords>en.stop</TrgStopWords>
	</stopwords>
	<dictionary>
		<Path>/mnt/data/babel/Resources/Dictionaries/ur-en/</Path>
		<SrcName>ur.tok.norm.dict</SrcName>
		<TrgName>en.tok.norm.dict</TrgName>
	</dictionary>
	<translit>
		<Path>/mnt/data/babel/Resources/Transliterations/ur-en/</Path>
		<SrcName>ur.translit</SrcName>
		<TrgName>en.translit</TrgName>
	</translit>
	<phrases>
   		<PhraseTable>/Users/aklement/Projects/Java/babel/phrase-table.ur-en.work.3</PhraseTable>
	</phrases>
</resources>
<preprocessing>
	<Path>./preprocessing/</Path>
	<FilterRomanSrc>false</FilterRomanSrc>
	<FilterRomanTrg>false</FilterRomanTrg>
	<input>
		<Context>crawls</Context>
		<Time>crawls</Time>
	</input>
	<context>
		<SrcEqClass>babel.content.eqclasses.SimpleEquivalenceClass</SrcEqClass>
		<TrgEqClass>babel.content.eqclasses.SimpleEquivalenceClass</TrgEqClass>
		<PruneEqIfOccursMoreThan>-1</PruneEqIfOccursMoreThan>
		<PruneEqIfOccursFewerThan>5</PruneEqIfOccursFewerThan>
		<PruneContextToSize>-1</PruneContextToSize>
		<Window>2</Window>
	</context>
	<candidates>
		<SrcEqClass>babel.content.eqclasses.SimpleEquivalenceClass</SrcEqClass>
		<TrgEqClass>babel.content.eqclasses.SimpleEquivalenceClass</TrgEqClass>
		<PruneIfOccursMoreThan>-1</PruneIfOccursMoreThan>
		<PruneIfOccursFewerThan>5</PruneIfOccursFewerThan>
		<PruneMostFrequentSrc>-1</PruneMostFrequentSrc>
		<PruneMostFrequentTrg>-1</PruneMostFrequentTrg>
	</candidates>
	<phrases>
		<MaxPhraseLength>7</MaxPhraseLength>
		<CaseSensitive>true</CaseSensitive>
	</phrases>
	<time>
		<Align>false</Align>
	</time>
</preprocessing>
<output>
	<Path>./output/</Path>
    <PhraseTable>phrase-table-mono</PhraseTable> 
    <ReorderingTable>reordering-table-mono</ReorderingTable> 
</output>
<experiments>
	<time>
		<SlidingWindow>false</SlidingWindow>
		<WindowSize>1</WindowSize>
	</time>
	<RandomSource>false</RandomSource>
	<NumSource>200</NumSource>
	<NumTranslationsToAddPerSource>500</NumTranslationsToAddPerSource>
	<DictionaryPruneNumTranslations>-1</DictionaryPruneNumTranslations>
	<NumRankingThreads>1</NumRankingThreads>	
	<DoTime>true</DoTime>
	<DoContext>true</DoContext>
	<DoEditDistance>true</DoEditDistance>
	<DoAggregate>true</DoAggregate>
</experiments>
</configuration>
